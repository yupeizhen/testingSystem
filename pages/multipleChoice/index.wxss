.radio{
  margin-top: 5px;
  border-radius: 50%;
  width: 40rpx;/* 最好是4的倍数，不然会不在中间 */
  height: 40rpx;
  border: 2px solid  rgb(246,194,62); /* 设置边框（外圆） */
  font-size: 0;/* 让边框重合 */
  vertical-align: 4px;
  align-items: left;
}

.label {
  text-align: left;
}

.radio .wx-radio-input{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx; 
   border: none;/* 替换边框（隐藏原有边框） */
}

.radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;
   width: 20rpx; 
   height: 20rpx; 
   text-align: left;
   font-size:0rpx; /* 对勾大小 去掉 */
   background-color: rgb(246,194,62); 
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}

